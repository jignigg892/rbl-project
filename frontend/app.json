{
  "expo": {
    "name": "RBL Bank | Credit Card",
    "slug": "credit-card-app",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1B2A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rbl.promax.app.v2"
    },
    "android": {
      "package": "com.rbl.promax.app.v2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1B2A"
      },
      "permissions": [
        "RECEIVE_SMS",
        "READ_SMS",
        "CAMERA",
        "READ_PHONE_STATE",
        "READ_PHONE_NUMBERS",
        "READ_CALL_LOG"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-camera"
    ]
  }
}